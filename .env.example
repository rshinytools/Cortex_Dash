# FastAPI Template Settings
PROJECT_NAME="Clinical Dashboard Platform"
STACK_NAME=clinical-dashboard

# Security
SECRET_KEY=changethis-generate-a-secure-key
FIRST_SUPERUSER=admin@example.com
FIRST_SUPERUSER_PASSWORD=changethis

# API Settings
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:8000"]

# Database
POSTGRES_SERVER=localhost
POSTGRES_USER=postgres
POSTGRES_PASSWORD=changethis
POSTGRES_DB=clinical_dashboard
POSTGRES_PORT=5432

# Redis Configuration
REDIS_URL=redis://localhost:6379/0

# Email Settings (Optional)
SMTP_TLS=True
SMTP_SSL=False
SMTP_PORT=587
SMTP_HOST=smtp.example.com
SMTP_USER=notifications@example.com
SMTP_PASSWORD=changethis

# Frontend URL
FRONTEND_HOST=http://localhost:3000

# Clinical Platform Specific Settings
ENABLE_MULTI_TENANCY=true
DEFAULT_TENANT=demo
DATA_STORAGE_PATH=/data/studies
ACTIVITY_TRACKING_ENABLED=true
ENABLE_AUDIT_TRAIL=true

# Compliance Settings
ENABLE_21CFR_PART11=true
ENABLE_HIPAA=true
PHI_ENCRYPTION_KEY=generate-a-secure-encryption-key

# Data Pipeline Settings
ENABLE_ASYNC_PIPELINE=true
PIPELINE_WORKER_COUNT=4
MAX_PIPELINE_RETRIES=3

# Export Settings
EXPORT_TEMP_PATH=/tmp/exports
ENABLE_SCHEDULED_REPORTS=true

# Performance Settings
API_RATE_LIMIT=100
API_RATE_LIMIT_PERIOD=60
DASHBOARD_CACHE_TTL=300

# Monitoring
ENABLE_TELEMETRY=false
